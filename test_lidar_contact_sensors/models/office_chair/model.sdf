<?xml version='1.0' encoding='utf-8'?>
<sdf version="1.6">
  <model name="OfficeChairGrey">
   <static>true</static>
    <link name="body">
      <pose>0 0 0 0 0 0</pose>

      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://models/office_chair/meshes/OfficeChairGrey.obj</uri>
          </mesh>
        </geometry>
      </visual>

      <collision name="collision">
        <geometry>
          <mesh>
            <uri>model://models/office_chair/meshes/OfficeChairGrey_Col.obj</uri>
          </mesh>
        </geometry>
      </collision>
      
      <sensor name='sensor_contact' type='contact'>
    <contact>
        <collision>collision</collision>
    </contact>
</sensor>

    </link>
    
    <plugin filename="gz-sim-touchplugin-system"
        name="gz::sim::systems::TouchPlugin">
    <target>vehicle_blue</target>
    <namespace>office_chair</namespace>
    <time>0.001</time>
    <enabled>true</enabled>
</plugin>
    
  </model>
</sdf>
