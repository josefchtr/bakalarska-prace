<?xml version="1.0" ?>
<sdf version="1.5">
  <model name="Sofa">
    <static>true</static>
    <link name="link">
      <pose>0 0 0 0 0 1.57079632679</pose>

        <visual name="visual">
          <geometry>
            <mesh><uri>model://models/sofa/meshes/Sofa.obj</uri></mesh>
          </geometry>
        </visual>

        <collision name="collision">
          <geometry>
            <mesh><uri>model://models/sofa/meshes/Sofa.obj</uri></mesh>
          </geometry>
        </collision>
  <sensor name='sensor_contact' type='contact'>
                    <contact>
                        <collision>collision</collision>
                    </contact>
          </sensor>
    </link>
    
    <plugin filename="gz-sim-touchplugin-system"
                    name="gz::sim::systems::TouchPlugin">
                <target>vehicle_blue</target>
                <namespace>Sofa</namespace>
                <time>0.001</time>
                <enabled>true</enabled>
    </plugin>
  </model>
</sdf>
