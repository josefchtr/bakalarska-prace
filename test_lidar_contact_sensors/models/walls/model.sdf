<?xml version="1.0"?>

<sdf version="1.6">
  <model name='walls'>
    <static>true</static>
    <link name="body">
      
        <visual name="visual">
          <geometry>
            <mesh><uri>model://models/walls/meshes/no_roof_UAMT.dae</uri></mesh>
          </geometry>
        </visual>

        <collision name="collision">
          <geometry>
            <mesh><uri>model://models/walls/meshes/no_roof_UAMT.dae</uri></mesh>
          </geometry>
        </collision>
 <sensor name='sensor_contact' type='contact'>
                    <contact>
                        <collision>collision</collision>
                    </contact>
                </sensor>
       
    </link>
    
    <plugin filename="gz-sim-touchplugin-system"
                    name="gz::sim::systems::TouchPlugin">
                <target>vehicle_blue</target>
                <namespace>wall</namespace>
                <time>0.001</time>
                <enabled>true</enabled>
            </plugin>
    
  </model>
</sdf>
